{
  "manifest_version": 2,

  "name": "Tokyo Night Google",
  "description": "Tokyo Night theme for Google",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Tokyo Night Google"
  },

  
  "content_scripts": [
    {
      "matches": ["*://*.google.com/*", "*://google.com/*"],
      "css": ["tokyo-night-google.css"]
    }
  ],
  "permissions": ["*://*.google.com/*", "*://google.com/*"]

}